<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd
       http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config />

    <bean id="currentDate" class="java.util.Date"/>

    <bean id="car7" class="edu.javacourse.spring.beans.Car7" scope="prototype">
        <property name="id" value="1"/>
        <property name="model" value="Subaru"/>
        <property name="year" value="2020"/>
        <property name="lastService" ref="currentDate"/>
    </bean>
</beans>